@model IEnumerable<ReservationBrute.Models.TableBooking>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.location)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.booked_seats)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.booking_no)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.date_time)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.booked_seats)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.booking_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date_time)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.id">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>
